<Window x:Class="PublicIPTrackerApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PublicIPTrackerApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="1047" Width="960"
        WindowStartupLocation="Manual" Left="-5" Top="0"
        >
    <Grid>
        <Menu x:Name="Menubar" Height="20" VerticalAlignment="Top">
            <MenuItem x:Name="MainPageNav" Header="Main window" Height="20" Click="MainPageNav_Click"/>
        </Menu>
        <Frame x:Name="Frame" Content="Frame" Margin="0,20,0,0" NavigationUIVisibility="Hidden" />
        <Button Content="Debugbutton, to get ip" HorizontalAlignment="Left" Margin="70,100,0,0" VerticalAlignment="Top" Height="34" Width="190" Click="Button_Click_2"/>
        <Button Content="Debugbutton to save to file" HorizontalAlignment="Left" Margin="172,172,0,0" VerticalAlignment="Top" Height="34" Width="187" Click="Button_Click_3"/>

    </Grid>
</Window>
